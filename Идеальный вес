package org.example;

public class Untitled {
    public static void main(String[] args) {
        int height = 170;
        int weight = 50;
        double idealWeight;
        idealWeight = (height - 100) - ((height - 100) * 0.10);
        if ((idealWeight - 5) < weight && (idealWeight + 5) > weight){
            System.out.println("Идеальный вес)))");
        }
        else{
            System.out.println("Плохой вес");
        }
    }
}
